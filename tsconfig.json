{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "temp",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "rootDir": ".",
    "paths": {
      "@tsenvar/*": [
        "./packages/*/src"
      ],
      "tsenvar": [
        "./packages/tsenvar/src"
      ]
    },
    "types": [
      "vitest/globals"
    ]
  },
  "include": [
    "packages/**/src/*.ts",
    "packages/core/src/internal/interface.ts",
    "packages/core/src/internal/index.ts",
    "examples",
    "packages/core/src/parser/parser.ts",
  ]
}
